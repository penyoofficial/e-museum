{
  "name": "penyo-db",
  "version": "1.0.0",
  "description": "A TypeScript-Based NoSQL database which provides basic functions for users.",
  "main": "index.ts",
  "scripts": {
    "dev": "tsc ./src/index.ts --outDir ./test --sourceMap true & node --inspect ./test/index.js",
    "build": "tsc ./src/index.ts --outDir ./temp & uglifyjs ./temp/*.js -o ./penyo-db.min.js & node ./penyo-db.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/penyoofficial/penyo-db.git"
  },
  "keywords": [
    "nosql",
    "database",
    "json"
  ],
  "author": "Penyo",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/penyoofficial/penyo-db/issues"
  },
  "homepage": "https://github.com/penyoofficial/penyo-db#readme",
  "devDependencies": {
    "@types/node": "^20.3.3",
    "inspect": "^0.0.2",
    "typescript": "^5.1.6",
    "uglify-js": "^3.17.4"
  }
}